            "type": "string",
            "format": "binary"
          }
        },
        "required": ["image"]
      },
      "SentimentAnalysisRequest": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          }
        },
        "required": ["text"]
      },
      "TextSummarizationRequest": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          }
        },
        "required": ["text"]
      },
      "ASRRequest": {
        "type": "object",
        "properties": {
          "audio": {
            "type": "string",
            "format": "binary"
          }
        },
        "required": ["audio"]
      },
      "TTSRequest": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          }
        },
        "required": ["text"]
      }
    }
