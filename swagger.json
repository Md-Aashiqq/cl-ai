{
  "openapi": "3.0.0",
  "info": {
    "title": "AI API",
    "description": "API for AI services",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.example.com"
    }
  ],
  "paths": {
    "/text/analyze": {
      "post": {
        "summary": "Analyze Text",
        "description": "Analyze text for sentiment and key entities",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "text": {
                    "type": "string",
                    "description": "Text to analyze"
                  }
                },
                "required": ["text"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Text analysis result",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "sentiment": {
                      "type": "string",
                      "description": "Sentiment of the text"
                    },
                    "entities": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "description": "Key entities identified in the text"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/image/classify": {
      "post": {
        "summary": "Classify Image",
        "description": "Classify an image into different categories",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "image": {
                    "type": "string",
                    "format": "binary",
                    "description": "Image file to classify"
                  }
                },
                "required": ["image"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Image classification result",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "categories": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "description": "Categories of the image"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
